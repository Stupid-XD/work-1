#include <iostream>
using namespace std;

int main() {
    setlocale(0, "");
    int size;
    char symbol;

    cout << "Фигура: \"Змейка\"" << endl << endl;

    cout << "Размер: ";
    cin >> size;

    cout << "Текстура: ";
    cin >> symbol;

    cout << endl << "\nРезультат:\n" << endl;

    for (int i = 1; i <= size; i++) {
        for (int j = 1; j <= 11; j++) {
            cout << symbol << " ";
        }
        cout << endl;
    }
    return 0;
}
